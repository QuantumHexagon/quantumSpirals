{
    // Style data id
    "id": "pl_100",

    "hue_min": 120,
    "hue_max": 120,
    "hue_ping_pong": false,
    "hue_increment": 0,

    // Pulse options
    "pulse_min": 0,
    "pulse_max": 1,
    "pulse_increment": 0.33,

    // 3D options
    "3D_depth": 1,
    "3D_skew": 0,
    "3D_spacing": 0,
    "3D_darken_multiplier": 1.1,
    "3D_alpha_multiplier": 0.4,
    "3D_alpha_falloff": 15.0,
    //"3D_override_color": [85, 95, 155, 255],
	"max_swap_time":0,

    // Main color
	// * The "main" tag is completely useless
	// * colors are not dynamic by default
	// * Pulse is by default [0, 0, 0, 0]
    "main": {"value": [0, 255, 0, 100], "pulse": [255, 0, 255, 155]},
    "cap_color": {"legacy": true, "index": 0},
	"text_color": {"value": [255, 255, 0, 255]},

    // Background colors
	// * Dynamic tags aren't needed if "dynamic" is false
    "colors":
    [
        //{"value": [0, 0, 0, 255]},
		//{"value": [0, 255, 0, 255]}
		
		{"dynamic": true, "dynamic_darkness": 10, "value": [0, 0, 0, 255]},
		{"dynamic": true, "dynamic_darkness": 2.5, "value": [0, 255, 0, 255]}

    ]
}
