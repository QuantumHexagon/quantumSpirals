{
    // Style data id
    "id": "pl_099",

    // Hue options
    "hue_min": 0,
    "hue_max": 360,
    "hue_ping_pong": false,
    "hue_increment": 1,

    // Pulse options
    "pulse_min": 0,
    "pulse_max": 1,
    "pulse_increment": 0.5,

    // 3D options
    "3D_depth": 1,
    "3D_skew": 0.15,
    "3D_spacing": 1,
	"3D_pulse_speed": 0.05,
    "3D_darken_multiplier": 1.1,
    "3D_alpha_multiplier": 0.4,
    "3D_alpha_falloff": 15.0,
    //"3D_override_color": [85, 95, 155, 255],
	
	"max_swap_time":0,

    // Main color
	// * The "main" tag is completely useless
	// * colors are not dynamic by default
	// * Pulse is by default [0, 0, 0, 0]
    "main": {"value": [128, 128, 128, 255], "pulse": [128, 128, 128, 255]},
    "cap_color": {"legacy": true, "index": 0},

    // Background colors
	// * Dynamic tags aren't needed if "dynamic" is false
    "colors":
    [
        {"dynamic": true, "dynamic_darkness": 2.5, "value": [255, 0, 0, 255]},
		{"dynamic": true, "dynamic_darkness": 3.0, "value": [255, 0, 0, 255]},
		{"dynamic": true, "dynamic_darkness": 3.5, "value": [255, 0, 0, 255]},
		{"dynamic": true, "dynamic_darkness": 4.0, "value": [255, 0, 0, 255]},
		{"dynamic": true, "dynamic_darkness": 4.5, "value": [255, 0, 0, 255]},
		{"dynamic": true, "dynamic_darkness": 5.0, "value": [255, 0, 0, 255]}

    ]
}
